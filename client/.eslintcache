= (id) => {\n    alert(id);\n  };\n\n  const cardList = products.allProduct.map((product) => {\n    return (\n      <Col md={3}>\n        <Card.Body\n          style={{ background: \"#fff\", margin: \"5px\" }}\n          key={product._id}\n        >\n          <Card.Title> {product.name} </Card.Title>\n          <Card.Subtitle className=\"mb-2 text-muted\">\n            {product.price}\n          </Card.Subtitle>\n          <Card.Text>{product.image}</Card.Text>\n          <Card.Link>\n            <Button\n              onClick={() => addToCartHandler(product)}\n              className=\"btn btn-danger\"\n            >\n              {add ? \"Adding\" : \" Add to Card\"}\n            </Button>\n          </Card.Link>\n          <Card.Link>\n            <Button\n              onClick={() => viewProductHandler(product._id)}\n              className=\"btn btn-xs btn-info\"\n            >\n              View\n            </Button>\n          </Card.Link>\n        </Card.Body>\n      </Col>\n    );\n  });\n  return (\n    <div>\n      <h3 className=\"text-center text-primary\">All Product</h3>\n      <Row>{cardList} </Row>\n    </div>\n  );\n};\n\nexport default Cart;\n","/home/ubed/Documents/expressjs/ecommerce-api/client/src/components/administrator/Header/BreadCrum.jsx",[],["145","146"],"/home/ubed/Documents/expressjs/ecommerce-api/client/src/apollos/queries/admin.js",[],["147","148"],"/home/ubed/Documents/expressjs/ecommerce-api/client/src/apollos/mutations/product.js",[],"/home/ubed/Documents/expressjs/ecommerce-api/client/src/components/Dashbaord/Header/Header.jsx",[],["149","150"],"/home/ubed/Documents/expressjs/ecommerce-api/client/src/components/Navigation/SignOutLink.jsx",[],"/home/ubed/Documents/expressjs/ecommerce-api/client/src/index.js",[],"/home/ubed/Documents/expressjs/ecommerce-api/client/src/reportWebVitals.js",[],"/home/ubed/Documents/expressjs/ecommerce-api/client/src/components/Dashbaord/Dashboard.jsx",[],"/home/ubed/Documents/expressjs/ecommerce-api